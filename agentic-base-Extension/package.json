{
  "name": "agentic-base-launcher",
  "displayName": "Agentic-Base Launcher",
  "description": "Initialize agentic-base in current workspace",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agentic-base-launcher.bootstrap",
        "title": "Agentic-Base: Bootstrap Current Project"
      }
    ],
    "configuration": {
      "title": "Agentic-Base Launcher",
      "properties": {
        "agenticBaseLauncher.sourcePath": {
          "type": "string",
          "default": "/Users/suzuki/Dev/agentic-base",
          "description": "Path to the agentic-base source repository"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.0"
  }
}